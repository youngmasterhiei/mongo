<h1 class="display-4">Article List:</h1>
<ul id="to-do-list" class="list-group">
  {{#each examples}}
  <li data-id="{{this.id}}" class="list-group-item">
    <a href="index{{this.id}}">{{this.text}}</a>
  </li>
  {{/each}}
  <br>
</ul>



<div class="row">
  <div class="col-7" id="articles" >
  </div>
  <div class="column-5" id="comments" >
   <form role="form">
      Username:
      <input type="text" id="userName">
      <br> Comment:
      <textarea id="textArea" type="text" ></textarea>
      <br>
      <input type="submit" id="commentSubmit" value="Submit">
    </form>
    <div id="commentList"></div>
  </div>
</div>

